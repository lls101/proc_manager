{
    "id": "user-api-service",
    "name": "java",
    "type": "service",
    "command": "/usr/bin/java",
    "args": [
    "-Xmx512m",
    "-jar",
    "/root/workspace/scripts/TimeLoop.jar"
  ],
    "workingDir": "/root/workspace/scripts",
    "pidFile": "my-java-app.pid",
    "autoStart": true,
    "healthCheck": {
        "enabled": true,
        "maxCpu": 80.0,
        "maxMem": 40.0
    }
}